{
  "name": "sample-nodejs",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "hieugiabao",
  "license": "MIT",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "build": "tsc && tsc -b",
    "dev": "tsc && tsc -b && nodemon -e ts --exec ts-node -r tsconfig-paths/register src/server.ts",
    "start": "tsc && tsc -b && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate:create": "typeorm migration:create",
    "migrate:run": "tsc && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migration.js up",
    "migrate:revert": "tsc && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migration.js down",
    "migrate:drop": "tsc && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migration.js drop",
    "migrate:show": "tsc && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migration.js show"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "ip": "^1.1.8",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^7.4.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "2.6.7",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.3",
    "pg-connection-string": "^2.6.2",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "signale": "^1.4.0",
    "swagger-express-ts": "^1.1.0",
    "swagger-ui-dist": "^5.4.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-typedi-extensions": "^0.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.9",
    "@types/signale": "^1.4.4",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^14.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "ts-node": "10.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.5.2"
  }
}
